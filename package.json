{
  "name": "js-green-licenses",
  "version": "0.1.0",
  "description": "JavaScript package.json license checker",
  "main": "build/src/checker.js",
  "bin": {
    "jsgl": "build/src/cli.js"
  },
  "types": "build/src/checker",
  "scripts": {
    "test": "ava",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli.js",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check && npm run check-self",
    "check-self": "node build/src/cli.js --local ."
  },
  "keywords": [],
  "author": "Google LLC",
  "license": "Apache-2.0",
  "ava": {
    "files": [
      "build/test/*-test.js"
    ]
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "axios": "^0.17.1",
    "npm-package-arg": "^6.0.0",
    "package-json": "^4.0.1",
    "spdx-correct": "^2.0.4",
    "spdx-satisfies": "^0.1.3"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.33",
    "@types/mock-fs": "^3.6.30",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.58",
    "@types/npm-package-arg": "^5.1.0",
    "@types/package-json": "^4.0.1",
    "@types/proxyquire": "^1.3.28",
    "@types/spdx-correct": "^2.0.0",
    "@types/spdx-satisfies": "^0.1.0",
    "ava": "^0.24.0",
    "gts": "^0.5.2",
    "mock-fs": "^4.4.2",
    "nock": "^9.1.4",
    "proxyquire": "^1.8.0",
    "typescript": "~2.6.2"
  }
}
